<resources xmlns:android="http://schemas.android.com/apk/res/android">

<style name="BlueOceanTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <item name="actionBarStyle">@style/CustomActionBar</item>
    <item name="homeAsUpIndicator">@drawable/title_return_selector</item>
    <item name="android:windowAnimationStyle">@style/BlueOceanTheme.Window</item>
</style>
    
	<!--Actionbar 样式-->
    <style name="CustomActionBar" parent="Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="background">@color/color_157cbe</item>
        <item name="titleTextStyle">@style/CustomActionBar.TitleTextColor</item>
    </style>

      <style name="CustomActionBar.TitleTextColor" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/white</item>
    </style>
    
      <!-- Activity动画  -->
    <style name="BlueOceanTheme.Window" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/in_from_right</item>
        <item name="android:activityOpenExitAnimation">@anim/out_from_left</item>
        <item name="android:activityCloseEnterAnimation">@anim/in_from_left</item>
        <item name="android:activityCloseExitAnimation">@anim/out_from_right</item>
    </style>
      
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">

    </style>


    <style name="AppTheme" parent="AppBaseTheme">
 
    </style>

    <style name="TextView14">
        <item  name="android:textSize">14sp</item>
        <item  name="android:textColor">#FFFFFF</item>
        <item name="android:background">@drawable/tv_bg_selector</item>
    </style>
    
    
</resources>
